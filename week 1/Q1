#include<bits/stdc++.h>
 using namespace std;

void linearsearch(vector<int> &arr,int n,int target )
{
  for(int i=0;i<n;i++)
  {
    if(arr[i]==target)
    {
      cout<<"Present at: "<<i;
      return;
    }

  }
  cout<<"Not present: "<<n;
}

int main()
{
  int tc;
  cin>>tc;
  while(tc--)
  {
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    int target;
    cin>>target;
    linearsearch(arr,n,target);
     tc--;
   }
   return 0;
  
 }
