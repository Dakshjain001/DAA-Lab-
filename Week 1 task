 #include<bits/stdc++.h>
 using namespace std;

Q.1 void linearsearch(vector<int> &arr,int n,int target )
{
  for(int i=0;i<n;i++)
  {
    if(arr[i]==target)
    {
      cout<<"Present at: "<<i;
      return;
    }

  }
  cout<<"Not present: "<<n;
}
int main()
{
  int tc;
  cin>>tc;
  while(tc--)
  {
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    int target;
    cin>>target;
     binarysearch(arr,n,target);
     tc--;
   }
   return 0;
  
 }


Q.2 int binarysearch(vector<int> &arr , int n,int target)
{
  int start=0;
  int end=n-1;
  int mid=start+(end-start)/2;
  while(start<=end)
  {
    if(arr[mid]==target)
    {
      cout<<"Element found at index: "<<mid;
      return 0;
    }
    if(arr[mid]>target)
    {
      end=mid-1;
    }
    else if(arr[mid]<target)
    {
      start=mid+1;
    }
    else{
      cout<<"Target not found";
      return -1;
    }

    mid=start+(end-start)/2;
  }
}
int main()
{
  int tc;
  cin>>tc;
  while(tc--)
  {
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    int target;
    cin>>target;
     binarysearch(arr,n,target);
     tc--;
   }
   return 0;
  
 }

Q.3 
void linearsearch(vector<int> &arr, int start, int end, int key);
void ispresent(vector<int> &arr, int key, int n);


int main()
{
  int tc;
  cin>>tc;
  while(tc--)
  {
    int n;
  cin>>n;
  vector<int>arr(n);
  for (int i = 0; i < n; i++)
  {
    cin>>arr[i];
  }
  int key;
  cin>>key;
  ispresent(arr,key,n);
  tc--;
  }
  
  return 0;
}

void ispresent(vector<int> &arr,int key,int n)
{
  if(arr[0]==key)
  {
    cout<<"present "<<0;
  }
  int i=1;
  while(i<n && arr[i]<=key)
  {
    i*=2;
  }
  linearsearch(arr,i/2,min(n-1,i),key);
}

void linearsearch(vector<int> &arr,int start,int end,int key)
{
  int c=0;
  for (int i = start; i <= end; i++)
  {
    if(arr[i]==key)
    {
      cout<<"Present at: "<<i<<endl;
      return;
    }
    else{
      c++;
    }
  }
  cout<<"Not present: "<<c;
  
  
}



